package model;

public class Livro {
	
	private String Autor;
	private String Tema;
	private int Quantpag;
	private boolean IsUsado;
	
	
	//getters and setters
	
	public String getAutor() {
		return Autor;
	}
	public void setAutor(String autor) {
		Autor = autor;
	}
	
	public String getTema() {
		return Tema;
	}
	
	public void setTema(String tema) {
		Tema = tema;
	}
	
	public int getQuantpag() {
		return Quantpag;
	}
	
	public void setQuantpag(int quantpag) {
		Quantpag = quantpag;
	}
	
	public boolean isIsUsado() {
		return IsUsado;
	}
	
	public void setIsUsado(boolean isUsado) {
		IsUsado = isUsado;
	}
	
	
	

	
	
	
}
