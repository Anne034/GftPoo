package funcionario;

public class Funcionario {
	
	private String Nome;
	private int Idade;
	protected double Salario;
	protected String getNome() {
		return Nome;
	}
	protected void setNome(String nome) {
		Nome = nome;
	}
	public int getIdade() {
		return Idade;
	}
	public void setIdade(int idade) {
		Idade = idade;
	}
	
	

}
