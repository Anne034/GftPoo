package model;

public abstract class Produto {
	
	private String Nome;
	private String Cnpj;
	
	
	
	public Produto()
	{
		
	}
	
	public Produto(String nome, double preco, int qtd) {
		
	}
	
	
	
	
	
	
	
	//get e set
	
	public String getNome() {
		return Nome;
	}
	public void setNome(String nome) {
		Nome = nome;
	}
	
	
	public String getCnpj() {
		return Cnpj;
	}
	public void setCnpj(String cnpj) {
		Cnpj = cnpj;
	}

	
	
	
}
